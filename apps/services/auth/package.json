    "nodemon": "^3.1.10",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "tsc-alias": "^1.8.16"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@prisma/client": "6.12.0",
    "@shipoff/proto": "workspace:*",
    "@shipoff/redis": "workspace:*",
    "@shipoff/services-commons": "workspace:*",
    "@shipoff/types": "workspace:*",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.1",
