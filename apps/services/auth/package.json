{
  "name": "auth",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/server.ts",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
